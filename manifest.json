{
    "name": "Ungrokker",
    "description": "Hides posts from those that @ an AI model in the replies",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
    },
    "content_scripts": [
        {
            "js": ["scripts/x.js"],
            "matches": [
                "*://*.twitter.com/*",
                "*://*.x.com/*"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ]
}